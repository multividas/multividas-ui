{
  "name": "@multividasdotcom/multividas-ui",
  "version": "1.0.1",
  "description": "Vue UI library",
  "keywords": [
    "multividas",
    "multividasdotcom",
    "threads"
  ],
  "author": "soulaimane yahya <contact@soulaimaneyahya.com> (https://soulaimaneyahya.com/)",
  "license": "MIT",
  "type": "module",
  "main": "multividas-ui.js",
  "module": "src/main.js",
  "types": "src/main.js",
  "exports": {
    ".": {
      "import": "./src/main.js",
      "require": "./multividas-ui.js"
    },
    "./dist/main.esm.js": "./src/main.js",
    "./dist/main.js": "./multividas-ui.js",
    "./dist/main.umd.js": "./multividas-ui.umd.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "engines": {
    "node": "^22.0.0"
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@babel/types": "^7.24.5",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vue-tsc": "^2.0.11"
  }
}